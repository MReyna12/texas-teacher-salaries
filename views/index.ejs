    <main>
      <section class="module-section-spacing state-bg-black">
        <div class="layout-container module-text-center">
          <div class="layout-teacher-flex module-space-between">
            <div class="layout-teacher-text">
              <div>
                <h1 class="module-h1 state-font-white">
                  Compare Teacher Salaries <br />
                  Across The State Of Texas<span class="state-period-color"
                    >.</span
                  >
                </h1>
              </div>
              <div>
                <p class="module-p-underH1Spacing state-font-white">
                  TTS provides teachers an easy way to view new hire salaries
                  <br />
                  for school districts in the metro areas of Austin, Dallas,
                  <br />
                  Houston, & San Antonio.
                </p>
              </div>
            </div>
            <div class="layout-flex layout-teacher-img-center">
              <div class="module-card">
                <img
                  class="module-main-teacher-img"
                  src="images/male-teacher2.png"
                  alt="Headshot of teacher"
                />
                <div class="module-card-container state-bg-white">
                  <h5>Michael Ramirez</h5>
                  <p class="module-card-teacher-spacing">English Teacher</p>
                  <p class="state-text-bold">HISD</p>
                </div>
              </div>
              <div>
                <div>
                  <img
                    class="module-teacher-img module-teacher1-img"
                    src="images/male-teacher.png"
                    alt="Headshot of teacher"
                  />
                </div>
                <div>
                  <img
                    class="module-teacher-img module-teacher2-img"
                    src="images/female-teacher2.png"
                    alt="Photo of teacher"
                  />
                </div>
                <div>
                  <img
                    class="module-teacher-img module-teacher3-img"
                    src="images/female-teacher3.png"
                    alt="Photo of teacher"
                  />
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
